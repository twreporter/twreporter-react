{
  "name": "twreporter-react",
  "version": "2.7.2",
  "description": "React-Redux site for The Reporter Foundation in Taiwan",
  "scripts": {
    "build": "npm run clean && npm run build-webpack && npm run build-server",
    "build-server": "NODE_ENV=production RELEASE_BRANCH=development BABEL_ENV=ssr babel src --out-dir dist --copy-files",
    "build-webpack": "NODE_ENV=production webpack --config webpack.config.js --progress --colors",
    "clean": "./node_modules/.bin/rimraf dist webpack-assets.json",
    "dev": "concurrently --kill-others \"npm run start-webpack-dev-server\" \"npm run start-dev-server\"",
    "lint": "eslint -c .eslintrc src",
    "start": "npm run build && npm run start-server",
    "start-dev-server": "API_HOST=dev-go-api.twreporter.org API_PORT=8080 API_PROTOCOL=http NODE_ENV=development RELEASE_BRANCH=development BABEL_ENV=ssr nodemon ./src/server --exec babel-node",
    "start-server": "NODE_ENV=production pm2 start processes.json",
    "close-server": "pm2 kill",
    "start-webpack-dev-server": "NODE_ENV=development node ./webpack-dev-server.js",
    "test": "./node_modules/mocha/bin/mocha $(find src -name '*.test.js') --compilers js:babel-core/register",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/twreporter/twreporter-react"
  },
  "license": "AGPL-3.0",
  "engines": {
    "node": ">=4.1.1"
  },
  "bugs": {
    "url": "https://github.com/twreporter/twreporter-react/issues"
  },
  "dependencies": {
    "@twreporter/react-components": "^4.0.5",
    "@twreporter/react-flex-carousel": "^1.1.1",
    "@twreporter/redux": "4.0.2",
    "@twreporter/registration": "2.2.1",
    "babel-plugin-inline-react-svg": "^0.4.0",
    "babel-plugin-system-import-transformer": "^3.1.0",
    "classnames": "^2.2.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.3.4",
    "es6-error": "^2.1.1",
    "express": "^4.13.3",
    "hoist-non-react-statics": "^2.5.0",
    "humps": "^0.6.0",
    "isomorphic-fetch": "^2.1.1",
    "lodash": "^4.0.0",
    "normalize.css": "^7.0.0",
    "normalizr": "^3.2.4",
    "piping": "^0.3.0",
    "pm2": "^2.2.3",
